{
  "compilerOptions": {
    "incremental": true,
    // "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "strict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "traceResolution": false,
    "listEmittedFiles": false,
    "listFiles": false,
    "pretty": true,
    "lib": [
      "ESNext",
      "ES2019",
      "ESNext.Array",
      "DOM"
    ],
    "baseUrl": "src",
    "paths": {
      "*": [
        "./*"
      ],
//      "*": [
//        "types/*"
//      ]
    },
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/*.ts",
    "src/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}